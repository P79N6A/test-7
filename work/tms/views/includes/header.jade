//- define the mixin
mixin menuItem(text, link, isActive)
	- if(isActive)
		li.active
			a(href= link)= text
	- else
		li
			a(href= link)= text


doctype

<html ng-app="!{header.ngApp}" class="!{iframe ? 'iframe' : ''}">
<head>
<meta charset="utf-8">
<title>!{header.title || 'TMS - 魔筷科技'}</title>
<link href="/assets/lib/bootstrap/css/bootstrap.min.css" rel="stylesheet">
<link href="/assets/mktms.css" rel="stylesheet">
<script src="/assets/lib/jquery.js"></script>
<script src="/assets/lib/jquery.cookie/cookie.js"></script>
<script src="/assets/lib/jquery.pagination.js"></script>
<script src="/assets/lib/bootstrap/js/bootstrap.js"></script>
<script src="/assets/lib/angular/angular.min.js"></script>
<script src="/assets/lib/angular/angular-sanitize.min.js"></script>
<script src="http://g.tbcdn.cn/kissy/k/1.4.1/seed-min.js"></script>
include ../mod/browserlimit
</head>
<body>

- if (!iframe)
	section.navlist
		ul
			- if(header.menu === 'personal')
				li.personal.active
					a(href="/personal")
						img.personal-image(src="data:image/png;base64,",alt="昵称")
						span.pid !{header.name}
			- else
				li.personal
					a(href="/personal")
						img.personal-image(src="data:image/png;base64,",alt="昵称")
						span.pid !{header.name}
			+menuItem('我的模板', '/temp', header.menu === 'temp')
			+menuItem('我的页面', '/page', header.menu === 'page')
			- if(header.permission === 'admin' || header.permission === 'coder')
				+menuItem('模块管理', '/mod', header.menu === 'mod')
			+menuItem('图片空间', '/img', header.menu === 'img')
			//- +menuItem('二维码', '/qrcode', header.menu === 'qrcode')
			//- +menuItem('活码平台', '/dynqrcode', header.menu === 'dynqrcode')
			- if(header.permission === 'admin')
				+menuItem('管理', '/s/admin', header.menu === 'admin')

			li.logout
				a(href="/logout") 退出
